     52          /*****************************************************************************
     53          * Tick_Diff
     54          *
     55          * Interface assumptions:
     56          *     BeforeTick    before tick
     57          *     NowTick       now tick
     58          *
     59          * Return value:
     60          *     uint32_t      differ value
     61          *
     62          ******************************************************************************/
     63          static uint32_t Tick_Diff( uint32_t BeforeTick, uint32_t NowTick )
     64          {
     65          	uint32_t rtnTick = 0;
     66          
     67          	if( BeforeTick <= NowTick )
     68          	{
     69          		rtnTick = (NowTick - BeforeTick) & 0xFFFFFFFF;
     70          	}
     71          	else if( BeforeTick > NowTick )
     72          	{
     73          		rtnTick = (NowTick + (0xFFFFFFFF - BeforeTick)) & 0xFFFFFFFF;
     74          	}
     75          
     76          	return rtnTick;
     77          }
