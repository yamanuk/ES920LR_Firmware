     79          /*****************************************************************************
     80          * CcaFreqConfig
     81          *
     82          * Interface assumptions:
     83          *     Ch            channel
     84          *     OrgBw         band width
     85          *     CcaBw         band width
     86          *     PlusMinusFreq shift frequency
     87          *
     88          * Return value:
     89          *     void
     90          *
     91          ******************************************************************************/
     92          static void CcaFreqConfig( uint8_t Ch, uint8_t OrgBw, uint8_t CcaBw, int32_t PlusMinusFreq )
     93          {
     94              uint32_t    RFFrequency;
     95              uint32_t    prvTick = 0;
     96          	uint32_t    nowTick = 0;
     97          
     98          
     99              // 周波数は、元々設定されている周波帯の中心周波数から計算
    100              switch( OrgBw )
    101              {
    102                  case 8:		// 250kHz幅
    103                      RFFrequency = 920700000 + (400000 * Ch) + PlusMinusFreq;
    104                      break;
    105          
    106                  case 9:		// 500kHz幅
    107                      RFFrequency = 920800000 + (600000 * Ch) + PlusMinusFreq;
    108                      break;
    109          
    110                  default:		// 125kHz幅以下
    111                      RFFrequency = 920600000 + (200000 * Ch) + PlusMinusFreq;
    112                      break;
    113              }
    114          
    115              SX1276LoRaSetRFFrequency( RFFrequency );
    116              SX1276LoRaSetSignalBandwidth( CcaBw ); // キャリアセンス向けに250kHzに変更
    117              prvTick = TPM0_CNT;
    118              do {
    119                  nowTick = TPM0_CNT;
    120              } while( Tick_Diff(prvTick, nowTick) < CCA_TIME );
    121          }
