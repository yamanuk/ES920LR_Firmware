     41          static void Timer_Init( void )
     42          {
     43          	SIM_SCGC6   |= SIM_SCGC6_TPM0_MASK;
     44          
     45              TPM0_SC     = 0x00;
     46              TPM0_CNT    = 0x00;
     47              TPM0_MOD    = 0xFFFF;
     48          
     49              TPM0_SC     |= 0x0C;
     50          }
