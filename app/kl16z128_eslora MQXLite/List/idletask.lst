     42          /*!
     43           * \brief This function is the code for the idle task.
     44           *
     45           * Idle Task is a MQX task that runs when all application tasks are blocked.
     46           * \n The function implements a simple counter. Size depends on the CPU
     47           * (64-bit counter for 16-bit CPUs, 128-bit counter for 32-bit CPUs).
     48           * \n Counter can be read from a debugger and idle CPU time can be calculated.
     49           *
     50           * \param[in] parameter Parameter passed to the task when created.
     51           */
     52          void _mqx_idle_task
     53              (
     54                  uint_32 parameter
     55              )
     56          { /* Body */
     57              volatile KERNEL_DATA_STRUCT_PTR kernel_data;
     58          
     59              _GET_KERNEL_DATA(kernel_data);
     60          
     61              while (1) {
     62          #if !defined(MQX_ENABLE_IDLE_LOOP) || MQX_ENABLE_IDLE_LOOP
     63                  if (++kernel_data->IDLE_LOOP.IDLE_LOOP1 == 0) {
     64                      if (++kernel_data->IDLE_LOOP.IDLE_LOOP2 == 0) {
     65                          if (++kernel_data->IDLE_LOOP.IDLE_LOOP3 == 0) {
     66                          ++kernel_data->IDLE_LOOP.IDLE_LOOP4;
     67                          } /* Endif */
     68                      } /* Endif */
     69                  } /* Endif */
     70          #endif
     71          #if MQX_ENABLE_LOW_POWER
     72                  if (parameter)
     73                  {
     74                      _ASM_SLEEP();
     75                  }
     76          #endif
     77          
     78              } /* Endwhile */
     79          
     80          } /* Endbody */
     81          #endif /* MQX_USE_IDLE_TASK */
